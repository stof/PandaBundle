<?xml version="1.0" ?>
<container xmlns="http://symfony.com/schema/dic/services"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="http://symfony.com/schema/dic/services http://symfony.com/schema/dic/services/services-1.0.xsd">

    <services>
        <service id="xabbuh_panda.panda_rest_client" class="%xabbuh_panda.client.rest.class%" public="false">
            <argument>%xabbuh_panda.cloud_id%</argument>
            <argument>%xabbuh_panda.access_key%</argument>
            <argument>%xabbuh_panda.secret_key%</argument>
            <argument>%xabbuh_panda.api_host%</argument>
        </service>
        <service id="xabbuh_panda.panda_api" class="%xabbuh_panda.client.api.class%" public="false">
            <argument type="service" id="xabbuh_panda.panda_rest_client"/>
        </service>
        <service id="xabbuh_panda.client" class="%xabbuh_panda.client.class%">
            <argument type="service" id="xabbuh_panda.panda_api"/>
            <argument type="service" id="xabbuh_panda.transformer"/>
        </service>
    </services>
</container>
