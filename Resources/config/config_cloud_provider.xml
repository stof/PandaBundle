<?xml version="1.0" ?>
<container xmlns="http://symfony.com/schema/dic/services"
           xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
           xsi:schemaLocation="http://symfony.com/schema/dic/services http://symfony.com/schema/dic/services/services-1.0.xsd">

    <services>
        <service id="xabbuh_panda.config_cloud_provider" class="%xabbuh_panda.cloud.config_provider.class%">
            <argument type="service" id="xabbuh_panda.cloud_manager"/>
            <argument type="service" id="xabbuh_panda.cloud_factory"/>
            <tag name="xabbuh_panda.cloud_provider"/>
        </service>
    </services>
</container>
